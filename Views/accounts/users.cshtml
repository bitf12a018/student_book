@model List<project_ead.Models.Account>

@{
    Layout = "~/Views/Shared/LayoutPg1.cshtml";
    }
<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  
 
  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="~/img/banner.jpg" alt="img" width="1500">
      <div class="carousel-caption">
          <h2>users list </h2>
      </div>
    </div>
    
    </div>
        </div>

          <!-- End course content -->



<br/><br/><br/>

<div class="container">
  @foreach(var s in Model)
    {
     
<div class="row" style="background-color:burlywood;">
    <div class="col-xs-12 col-sm-6 col-lg-8"><h3>name : @s.name</h3></div>
    <div class="col-xs-6 col-lg-4"><h3>email : @s.email</h3> </div>
  </div>
  <div class="row" style="background-color:bisque;">
    <div class="col-xs-12 col-sm-6 col-lg-8"><h3>User name :  @s.username</h3></div>
    
  </div> 
      
      <br/>
         if(Session["login_admin"]!=null)
         {

      <div>

   <a href="/accounts/update_user/@s.Id">Update</a>
    <a href="/accounts/delete_user/@s.Id">Delete</a>
 

    </div>
         }
      <br/><br/><br/>
}
    </div>
