@{
    Layout = "~/Views/Shared/LayoutPg1.cshtml";
    }


     <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="~/img/banner.jpg" alt="img" width="1500"/>
      <div class="carousel-caption">
          <h2>uploads edits</h2>
      </div>
    </div>
    
    </div>
          <!-- End  content -->


    <div class="container">

  <!-- Modal -->
  <div class="modal fade" id="event_Model" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">upload event</h4>
        </div>
        <div class="modal-body">
            <!--Here is body of Model-->
         <strong> event type: </strong> @Html.TextBox("event_type") <br/><br />
         <strong> Event detail:</strong>  @Html.TextBox("event_detail")  <br/><br/>
          <strong> Event date:</strong>  <input type="date" name="event_date" /> <br/><br/>

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal" onclick="goToStudentHome()">upload</button>
        </div>
      </div>
      
    </div>
  </div>
  
</div>

